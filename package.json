{
  "name": "cdn-resolve",
  "version": "2.1.2",
  "description": "Resolves import paths to their CDN equivalents",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.20.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/cdn-resolve",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/cdn-resolve.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/cdn-resolve/issues"
  },
  "keywords": [
    "resolve",
    "cdn",
    "import",
    "unpkg",
    "jsdelivr",
    "esm.sh"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./esm": {
      "import": "./dist/esm.mjs",
      "require": "./dist/esm.cjs"
    },
    "./jsdelivr": {
      "import": "./dist/jsdelivr.mjs",
      "require": "./dist/jsdelivr.cjs"
    },
    "./parse": {
      "import": "./dist/parse.mjs",
      "require": "./dist/parse.cjs"
    },
    "./unpkg": {
      "import": "./dist/unpkg.mjs",
      "require": "./dist/unpkg.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "6.0.3",
    "@types/node": "22.15.30",
    "eslint": "9.39.1",
    "eslint-plugin-format": "1.0.2",
    "publint": "0.3.15",
    "tsdown": "0.16.1",
    "typescript": "5.9.3",
    "vitest": "4.0.8"
  },
  "pnpm": {
    "overrides": {
      "rolldown": "1.0.0-beta.47"
    }
  }
}
