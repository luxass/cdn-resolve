{
  "name": "cdn-resolve",
  "version": "0.1.5",
  "description": "Resolves import paths to their CDN equivalents",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@8.14.1",
  "license": "MIT",
  "homepage": "https://github.com/luxass/cdn-resolve",
  "repository": "https://github.com/luxass/cdn-resolve",
  "bugs": {
    "url": "https://github.com/luxass/cdn-resolve/issues"
  },
  "keywords": [
    "resolve",
    "cdn",
    "import",
    "skypack",
    "unpkg",
    "jsdelivr",
    "esm.sh"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./skypack": {
      "types": "./dist/skypack.d.ts",
      "import": "./dist/skypack.js",
      "require": "./dist/skypack.cjs"
    },
    "./esm": {
      "types": "./dist/esm.d.ts",
      "import": "./dist/esm.js",
      "require": "./dist/esm.cjs"
    },
    "./jsdelivr": {
      "types": "./dist/jsdelivr.d.ts",
      "import": "./dist/jsdelivr.js",
      "require": "./dist/jsdelivr.cjs"
    },
    "./unpkg": {
      "types": "./dist/unpkg.d.ts",
      "import": "./dist/unpkg.js",
      "require": "./dist/unpkg.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "esm.d.ts",
    "jsdelivr.d.ts",
    "skypack.d.ts",
    "unpkg.d.ts"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ofetch": "^1.3.3"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.2.1",
    "@types/node": "^18.16.19",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
