{
  "name": "cdn-resolve",
  "type": "module",
  "version": "0.1.4",
  "description": "Resolves import paths to their CDN equivalents",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/cdn-resolve",
  "repository": "https://github.com/luxass/cdn-resolve",
  "bugs": {
    "url": "https://github.com/luxass/cdn-resolve/issues"
  },
  "keywords": [
    "resolve",
    "cdn",
    "import",
    "skypack",
    "unpkg",
    "jsdelivr",
    "esm.sh"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./skypack": {
      "import": "./dist/skypack.js",
      "require": "./dist/skypack.cjs",
      "types": "./dist/skypack.d.ts"
    },
    "./esm": {
      "import": "./dist/esm.js",
      "require": "./dist/esm.cjs",
      "types": "./dist/esm.d.ts"
    },
    "./jsdelivr": {
      "import": "./dist/jsdelivr.js",
      "require": "./dist/jsdelivr.cjs",
      "types": "./dist/jsdelivr.d.ts"
    },
    "./unpkg": {
      "import": "./dist/unpkg.js",
      "require": "./dist/unpkg.cjs",
      "types": "./dist/unpkg.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "skypack.d.ts",
    "esm.d.ts",
    "jsdelivr.d.ts",
    "unpkg.d.ts"
  ],
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "tsup",
    "dev": "vitest dev",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ofetch": "^1.0.1"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.1.1",
    "@luxass/prettier-config": "^1.2.0",
    "@types/node": "^18.15.10",
    "eslint": "^8.36.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vitest": "^0.31.0"
  }
}
