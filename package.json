{
  "name": "cdn-resolve",
  "type": "module",
  "version": "0.1.0",
  "description": "Resolves import paths to their CDN equivalents",
  "author": {
    "name": "Lucas NÃ¸rgÃ¥rd",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/cdn-resolve",
  "repository": "https://github.com/luxass/cdn-resolve",
  "bugs": {
    "url": "https://github.com/luxass/cdn-resolve/issues"
  },
  "keywords": [
    "resolve",
    "cdn",
    "import",
    "skypack",
    "unpkg",
    "jsdelivr",
    "esm.sh"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./skypack": {
      "types": "./dist/skypack.d.ts",
      "require": "./dist/skypack.cjs",
      "import": "./dist/skypack.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.29.0",
  "scripts": {
    "build": "tsup",
    "dev": "vitest dev",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.1.0",
    "@luxass/prettier-config": "^1.2.0",
    "@types/node": "^18.15.10",
    "eslint": "^8.36.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  }
}
