{
  "name": "cdn-resolve",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@8.7.5",
  "description": "Resolves import paths to their CDN equivalents",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/cdn-resolve",
  "repository": "https://github.com/luxass/cdn-resolve",
  "bugs": {
    "url": "https://github.com/luxass/cdn-resolve/issues"
  },
  "keywords": [
    "resolve",
    "cdn",
    "import",
    "skypack",
    "unpkg",
    "jsdelivr",
    "esm.sh"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./skypack": {
      "types": "./dist/skypack.d.ts",
      "require": "./dist/skypack.cjs",
      "import": "./dist/skypack.js"
    },
    "./esm": {
      "types": "./dist/esm.d.ts",
      "require": "./dist/esm.cjs",
      "import": "./dist/esm.js"
    },
    "./jsdelivr": {
      "types": "./dist/jsdelivr.d.ts",
      "require": "./dist/jsdelivr.cjs",
      "import": "./dist/jsdelivr.js"
    },
    "./unpkg": {
      "types": "./dist/unpkg.d.ts",
      "require": "./dist/unpkg.cjs",
      "import": "./dist/unpkg.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "skypack.d.ts",
    "esm.d.ts",
    "jsdelivr.d.ts",
    "unpkg.d.ts"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "dev": "vitest",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ofetch": "^1.1.1"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.2.2",
    "@types/node": "^18.16.19",
    "eslint": "^8.44.0",
    "tsup": "^8.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.0"
  }
}
